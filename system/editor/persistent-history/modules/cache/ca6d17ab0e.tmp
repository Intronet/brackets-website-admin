
.preference-dialog .spinner.spin {
-webkit-animation: spinner-rotateplane 1.2s 2 ease-in-out;
animation: spinner-rotateplane 1.2s 2 ease-in-out;
visibility: visible;
}

#upDown{
	text-align: center !important;
}
