   margin-bottom: 0;
   background-image: none;
   width: 47px;
}
.jstree-brackets li a:hover {
   color: #8fddff;
   background-color: #666;
}
div#ftpTree li a.active {
   color: #fff;
   background-color: #2d2e30;
}
div#ftpTree li a {
   margin-right: 1px;
}
.modal .nav-tabs {
   border-bottom: 1px solid #343434;
}
.modal .nav-tabs > a {}
.modal .nav-tabs > .active > a {
   background-color: #dfe2e2 !important;
   border: 1px solid #343434;
   border-bottom: 1px solid #dfe2e2 !important;
}
.dark .modal .nav-tabs {
   border-bottom: 1px solid #343434;
}
.dark .modal .nav-tabs a {
   color: #cccccc;
   border: 1px solid transparent;
}
.dark .modal .nav-tabs a:hover {
   background: transparent;
   color: #6bbeff;
   outline: none;
   box-shadow: none;
}
.dark .modal .nav-tabs a:focus {
   outline: none;
   box-shadow: none;
}
.dark .modal .nav-tabs > .active > a {
   background-color: #2c2c2c !important;
   border: 1px solid #413F3F;
   border-bottom: 1px solid #2c2c2c !important;
}
li#ftpTree-context-menu, li#project-context-menu {
